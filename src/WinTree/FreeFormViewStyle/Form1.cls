	METHOD PRIVATE VOID InitializeDataSet(  ):
        employeeService = NEW EmployeeService().
        employeeService:fetchData().
        bindingSource1:HANDLE = employeeService:DatasetHandle.
        
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID Form1_Load( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        InitializeDataSet().

		/*Populate the Patients tree. This tree demonstrates using FreeForm ViewStyle to
		  display child nodes with different ColumnSets.*/
		PopulateTreeWithEmployeeData().

		/*Load the Animals tree. This tree loads data from XML where each node as
		  different attribute, and therefore displays a different ColumnSet.*/ 
	    PopulateTreeWithAnimalData().

	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PRIVATE VOID PopulateTreeWithEmployeeData(  ):
	    DEFINE VARIABLE employeeNode   AS UltraTreeNode NO-UNDO.
	    DEFINE VARIABLE addressNode    AS UltraTreeNode NO-UNDO.
	    DEFINE VARIABLE statsNode      AS UltraTreeNode NO-UNDO.
	    DEFINE VARIABLE employeeBuffer AS HANDLE        NO-UNDO.
	    DEFINE VARIABLE totalRecords   AS INTEGER       NO-UNDO.
	    DEFINE VARIABLE currentRecord  AS INTEGER       NO-UNDO.

	    treeEmployees:ViewStyle = Infragistics.Win.UltraWinTree.ViewStyle:FreeForm.

   		/*Assign the "Employees" ColumnSet to all nodes in Band 0.*/
		treeEmployees:NodeLevelOverrides[0]:ColumnSet = treeEmployees:ColumnSettings:ColumnSets["Employees"].

        ASSIGN totalRecords   = bindingSource1:COUNT - 1
               employeeBuffer = bindingSource1:handle::eEmployee.

        REPEAT currentRecord = 0 TO totalRecords:
            bindingSource1:POSITION = currentRecord.

            /*Create a employee node for this row of data. The Key of the node
		      will be the "EmpNum" field.*/ 
		    employeeNode = treeEmployees:Nodes:Add(STRING(employeeBuffer::EmpNum)).
		    /*Populate the three fields defined in the ColumnSet.*/ 
		    employeeNode:Cells:Item["FirstName"]:Value = employeeBuffer::FirstName.
		    employeeNode:Cells:Item["LastName"]:Value = employeeBuffer::LastName.

		    /*Under the employee node, add an address node.*/ 
		    addressNode = employeeNode:Nodes:Add().
		    addressNode:OVERRIDE:ColumnSet = treeEmployees:ColumnSettings:ColumnSets["Address"].
		    addressNode:Cells["Address"]:Value = employeeBuffer::Address.
		    addressNode:Cells["City"]:Value = employeeBuffer::City.
		    addressNode:Cells["State"]:Value = employeeBuffer::State.
		    addressNode:Cells["PostalCode"]:Value = employeeBuffer::PostalCode.
		    addressNode:Cells["HomePhone"]:Value = employeeBuffer::HomePhone.

		    /*Create a stats node under the employee node.*/ 
			statsNode = employeeNode:Nodes:Add().

			/*Just like the address node, the stats node will have a sibling that does
			  not use the same ColumnSet. So we need to set the ColumnSet on the override 
			  of each stats node.*/ 
			statsNode:OVERRIDE:ColumnSet = treeEmployees:ColumnSettings:ColumnSets["Stats"].
			statsNode:Cells["BirthDate"]:Value = employeeBuffer::BirthDate.
			statsNode:Cells["StartDate"]:Value = employeeBuffer::StartDate.
			statsNode:Cells["VacationDaysLeft"]:Value = employeeBuffer::VacationDaysLeft.
		    statsNode:Cells["SickDaysLeft"]:Value = employeeBuffer::SickDaysLeft.
        END.

        treeEmployees:ExpandAll().
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PRIVATE VOID PopulateTreeWithAnimalData(  ):
	    DEFINE VARIABLE xmlDoc                AS XmlDocument                    NO-UNDO.
        DEFINE VARIABLE rootElement           AS XmlElement                     NO-UNDO.
        DEFINE VARIABLE rootNode              AS UltraTreeNode                  NO-UNDO.
        DEFINE VARIABLE categoryElement       AS XmlElement                     NO-UNDO.
        DEFINE VARIABLE childNodes            AS System.Collections.IEnumerator NO-UNDO.
        DEFINE VARIABLE totalChildNodes       AS INTEGER                        NO-UNDO.
        DEFINE VARIABLE category              AS CHARACTER                      NO-UNDO.
        DEFINE VARIABLE categoryNode          AS UltraTreeNode                  NO-UNDO.
        DEFINE VARIABLE animalElement         AS XmlElement                     NO-UNDO.
        DEFINE VARIABLE categoryChildrenNodes AS System.Collections.IEnumerator NO-UNDO.
        DEFINE VARIABLE columnSet             AS UltraTreeColumnSet             NO-UNDO.
        DEFINE VARIABLE animalNode            AS UltraTreeNode                  NO-UNDO.
        DEFINE VARIABLE attribute             AS XmlAttribute                   NO-UNDO.
        DEFINE VARIABLE animalAttributes      AS System.Collections.IEnumerator NO-UNDO.
        DEFINE VARIABLE nodeColumn            AS UltraTreeNodeColumn            NO-UNDO.

	    /*Load the XML document containing the data*/
		xmlDoc = new XmlDocument().
		xmlDoc:Load(SEARCH("WinTree/FreeFormViewStyle/Animals.xml")).

		/*Get the root element*/
		rootElement = xmlDoc:DocumentElement.

		/*Set the ViewStyle of the this.treeAnimals to FreeForm*/
		treeAnimals:ViewStyle = Infragistics.Win.UltraWinTree.ViewStyle:FreeForm.

		/*Clear any existing nodes in the tree*/
		treeAnimals:Nodes:Clear().

		/*Add a Root node to the tree representing the root Element in the XML*/
	    rootNode = treeAnimals:Nodes:Add(?, rootElement:Name).

		/*Loop through the child nodes of the root element*/
		childNodes = rootElement:ChildNodes:GetEnumerator().
        childNodes:RESET().

        REPEAT:
            IF NOT childNodes:MoveNext() THEN LEAVE.
            categoryElement = CAST(childNodes:current, XmlElement).

			/*Get the category attribute on each child node*/
			category = categoryElement:GetAttribute("category").

			/*Create a category node in the tree to represent the category of the xml node*/
			categoryNode = rootNode:Nodes:Add(?, category).

			/*Loop through the child elements of the Category node.*/
			categoryChildrenNodes = categoryElement:ChildNodes:GetEnumerator().
			categoryChildrenNodes:RESET().

			REPEAT:
			    IF NOT categoryChildrenNodes:MoveNext() THEN LEAVE.
			    
			    animalElement = CAST(categoryChildrenNodes:current, XmlElement).

				/*Create a ColumnSet based on the Attributes of this element
				  Each attribute will represent a column.*/
			    columnSet = new UltraTreeColumnSet().

				/*The ColumnSet key will be the animal type.*/
				columnSet:Key = animalElement:GetAttribute("type").
				columnSet:COLUMNS:Add(columnSet:Key).

				/*Add the new ColumnSet into the ColumnSets collection*/
				treeAnimals:ColumnSettings:ColumnSets:Add(columnSet).

				/*Create a tree node to represent the XML animal node*/
				animalNode = categoryNode:Nodes:Add(?, animalElement:Name).

				/*Assign the newly created ColumnSet to this node.*/ 
				animalNode:OVERRIDE:ColumnSet = columnSet.

				/*Loop through the attributes of the animal xml node.*/
				animalAttributes = animalElement:Attributes:GetEnumerator().
				animalAttributes:RESET().

                /*animalNode:Cells[columnSet:Key]:Value = animalElement:GetAttribute("type").*/
				REPEAT:
				    IF NOT animalAttributes:MoveNext() THEN LEAVE.

                    ASSIGN attribute = CAST(animalAttributes:current, XmlAttribute).

					/*For each attribute, create a Column in the ColumnSet.*/ 
					nodeColumn = columnSet:COLUMNS:Add(attribute:Name).

					/*Populate the tree node's cell with the appropriate value.*/
					animalNode:Cells[attribute:Name]:Value = attribute:Value.

					/*AutoResize the column so it fits the text*/
					nodeColumn:PerformAutoResize(Infragistics.Win.UltraWinTree.ColumnAutoSizeMode:AllNodesWithDescendants).
				END. /*animalAttributes*/

			END. /*categoryChildrenNodes*/
		END. /*REPEAT childNodes*/			

		/*Expand all the nodes*/
		treeAnimals:ExpandAll().
	END METHOD.
END CLASS.