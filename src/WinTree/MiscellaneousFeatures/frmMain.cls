	@VisualDesigner.
	METHOD PRIVATE VOID button1_Click( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
	    DEFINE VARIABLE controlPanel         AS frmControlPanel             NO-UNDO.
	    DEFINE VARIABLE featureItem          AS NewFeatureItem              NO-UNDO.
	    DEFINE VARIABLE currentScreen        AS System.Windows.Forms.Screen NO-UNDO.
	    DEFINE VARIABLE controlPanelPosition AS INTEGER                     NO-UNDO.

	    IF ultraColorPicker1:Parent    NE THIS-OBJECT THEN ultraColorPicker1:Parent    = THIS-OBJECT.
		IF ultraDateTimeEditor1:Parent NE THIS-OBJECT THEN ultraDateTimeEditor1:Parent = THIS-OBJECT.
		IF ultraFontNameEditor1:Parent NE THIS-OBJECT THEN ultraFontNameEditor1:Parent = THIS-OBJECT.

		controlPanel = new frmControlPanel().
	    featureItem = CAST(listBox1:SelectedItem, NewFeatureItem).

		/*If the form is maximized, then we don't want to show the control panel form to the right of it, since
		  it will then appear off-screen*/
	    currentScreen = System.Windows.Forms.Screen:FromRectangle(new System.Drawing.Rectangle(THIS-OBJECT:Location, THIS-OBJECT:Size)).
	    controlPanelPosition = THIS-OBJECT:Location:X + THIS-OBJECT:WIDTH.
		IF controlPanelPosition GE currentScreen:Bounds:Width THEN
		    controlPanel:ShowMe(THIS-OBJECT,ultraTree1,featureItem,this-object:Location).
		ELSE
		    controlPanel:ShowMe(THIS-OBJECT, ultraTree1, featureItem, new System.Drawing.Point(controlPanelPosition, THIS-OBJECT:Location:Y)).
	END METHOD.

