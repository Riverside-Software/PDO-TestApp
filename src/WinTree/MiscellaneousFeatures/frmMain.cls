	@VisualDesigner.
	METHOD PRIVATE VOID frmMain_Load( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        PopulateNewFeaturesList().

		/*Set the SelectionType property to Extended to ENABLE multi-select capability.*/
		ultraTree1:OVERRIDE:SelectionType = SelectType:EXTENDED.

		/*Set HideSelection to false so the selected nodes
		  are visually apparent when the tree does not have focus.*/
		ultraTree1:HideSelection = FALSE.

		/*Set LabelEdit to DefaultableBoolean.True so nodes can	go into edit mode*/
		ultraTree1:OVERRIDE:LabelEdit = DefaultableBoolean:TRUE.

		PopulateTree(TRUE).
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PRIVATE VOID PopulateNewFeaturesList(  ):
        listBox1:Items:Clear(). 
		listBox1:Items:Add( new NewFeatureItem( NewFeatureType:CutCopyPaste)).
		listBox1:Items:Add( new NewFeatureItem( NewFeatureType:EmbeddableEditors)).
		listBox1:Items:Add( new NewFeatureItem( NewFeatureType:InterNodeSpacing)).
		listBox1:Items:Add( new NewFeatureItem( NewFeatureType:MultilineNodes)).
		listBox1:Items:Add( new NewFeatureItem( NewFeatureType:MaxLabelSizing)).
		listBox1:Items:Add( new NewFeatureItem( NewFeatureType:SavingLoadingLayouts)).
		listBox1:SelectedIndex = 0.
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PRIVATE VOID PopulateTree(INPUT expandAll AS LOGICAL):
	    DEFINE VARIABLE i             AS INTEGER       NO-UNDO.
	    DEFINE VARIABLE j             AS INTEGER       NO-UNDO.
	    DEFINE VARIABLE k             AS INTEGER       NO-UNDO.
        DEFINE VARIABLE rootNodeText  AS CHARACTER     NO-UNDO.
        DEFINE VARIABLE childNodeText AS CHARACTER     NO-UNDO.
        DEFINE VARIABLE rootNode      AS UltraTreeNode NO-UNDO.
        DEFINE VARIABLE childNode     AS UltraTreeNode NO-UNDO.

	    ultraTree1:Nodes:Clear().

        REPEAT i = 1 TO 10:
		    rootNodeText = "Root Node " + STRING(i).
			rootNode = ultraTree1:Nodes:Add("", rootNodeText + GetMultilineData(3)).
			
			REPEAT j = 1 TO 10:
			    childNodeText = rootNodeText + " Child " + STRING(j).
				childNode = rootNode:Nodes:Add("", childNodeText + GetMultilineData(3)).
				
				REPEAT k = 1 TO 10:
				    childNode:Nodes:Add("", childNodeText + " Grandchild " + STRING(k) + GetMultilineData(3)).
				END.
			END.
        END.
		IF expandAll THEN ultraTree1:ExpandAll().
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PUBLIC CHARACTER GetMultilineData(INPUT numberOfLines AS INTEGER):
	    DEFINE VARIABLE crlf   AS CHARACTER NO-UNDO.
	    DEFINE VARIABLE retVal AS CHARACTER NO-UNDO.
        DEFINE VARIABLE i      AS INTEGER   NO-UNDO.

        ASSIGN crlf = CHR(10).

        REPEAT i = 2 TO numberOfLines:
		    retVal = retVal + crlf + "Line " + STRING(i).
		END.

		RETURN retVal.
	END METHOD.

