	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PRIVATE VOID UpdateFeatureDescription():
	    DEFINE VARIABLE descriptn    AS CHARACTER NO-UNDO.
        DEFINE VARIABLE crlf         AS CHARACTER NO-UNDO.
        DEFINE VARIABLE selectedItem AS NewFeatureItem NO-UNDO.

        crlf = CHR(13) + CHR(10).
		selectedItem = CAST(listBox1:SelectedItem, NewFeatureItem).

		IF selectedItem = ? THEN RETURN.

		CASE selectedItem:FeatureType:
		
		    WHEN NewFeatureType:CutCopyPaste THEN
            DO:
				descriptn = "The UltraWinTree control now supports the cutting, copying, and pasting of nodes." + crlf + crlf.
				descriptn = descriptn + "Three properties are exposed that determine whether a node can be cut, copied, or pasted: AllowCut, AllowCopy, and AllowPaste, respectively." + crlf.
				descriptn = descriptn + crlf + frmMain:OVERRIDE_LEVEL_PROPERTY_DESCRIPTION.
				descriptn = descriptn + "When set to DefaultableBoolean.True, the property enables the corresponding operation via keystrokes; the keystrokes that are assigned to perform these operations correspond directly to the standard Windows keystrokes (e.g., Ctrl+C copies the selected nodes), but can be reassigned to any key by modifying their entries in the control's KeyActionMappings collections" + crlf + crlf.
				descriptn = descriptn + "Additionally, methods are exposed at the control level that enable the end developer to perform the same operations programmatically; these methods are CutSelectedNodes, CopySelectedNodes, and PasteNodes. These methods do not observe the resolved values of their corresponding 'Allow' properties.".
			END.

			WHEN NewFeatureType:EmbeddableEditors THEN
            DO:
				descriptn = "The UltraWinTree control now supports embeddable editors for editing and rendering node text.".
				descriptn = descriptn + "Two properties are exposed that determine which custom embeddable editor is used to render and edit the node's Text: Editor, and EditorControl." + crlf.
				descriptn = descriptn + crlf + frmMain:OVERRIDE_LEVEL_PROPERTY_DESCRIPTION.
				descriptn = descriptn + "When assigned, the nodes affected by that Override use that editor to render and edit the node's Text." + crlf + crlf.
				descriptn = descriptn + "Additionally, properties are exposed to determine whether custom editors are used, as well as whether and how their edit buttons are displayed. These properties are UseEditor and ShowEditorButtons, respectively. Like the Editor and EditorControl properties, they are exposed by the Override object." + crlf + crlf.
				descriptn = descriptn + "For a more detailed discussion of embeddable editors and how they work with the UltraWinTree control, see the 'UltraTree EmbeddableEditors' sample.".
			END.
				
			WHEN NewFeatureType:InterNodeSpacing THEN
            DO:
				descriptn = "The UltraWinTree control now exposes 2 properties that allow the end developer to control the amount of vertical spacing before and after the node: NodeSpacingBefore and NodeSpacingAfter." + crlf.
				descriptn = descriptn + "These properties can be used to visually separate one node from its siblings, as well as create virtual groups of nodes." + crlf.
				descriptn = descriptn + crlf + frmMain:OVERRIDE_LEVEL_PROPERTY_DESCRIPTION.
			END.

			WHEN NewFeatureType:MultilineNodes THEN
            DO:
				descriptn = "The UltraWinTree control now supports multiline node text." + crlf.
				descriptn = descriptn + "The Multiline property, exposed by the Override object, determines whether a node's Text will be displayed with multiple lines." + crlf.
				descriptn = descriptn + "When the node's Text contains explicit newline characters, text following the newline character will wrap to the next line." + crlf + crlf.
				descriptn = descriptn + "The Override object also exposes a property that constrains the width of the label portion of the node (see MaxLabelWidth)." + crlf.
				descriptn = descriptn + "When the MaxLabelWidth property is set to a value that is less than the horizontal space required to fully display the node's Text, the text will wrap to the next line." + crlf.
			END.

			WHEN NewFeatureType:MaxLabelSizing THEN
            DO:
				descriptn = "The UltraWinTree control now supports constraining the size of the label portion of the node." + crlf.
				descriptn = descriptn + "Two properties are exposed that determine the maximum size of the node's label portion: MaxLabelWidth and MaxLabelHeight." + crlf.
				descriptn = descriptn + crlf + frmMain:OVERRIDE_LEVEL_PROPERTY_DESCRIPTION.
				descriptn = descriptn + "When set to a value that is greater than 0, the size label portion of the node will be constrained by that amount." + crlf.
			END.

			WHEN NewFeatureType:SavingLoadingLayouts THEN
            DO:
				descriptn = "The UltraWinTree control now supports saving and loading layouts to persist the state of the control." + crlf + crlf.
				descriptn = descriptn + "Four methods are exposed by the control that enable the end developer to save and load the state of the control: SaveAsBinary, SaveAsXml, LoadFromBinary, and LoadFromXml." + crlf.
			END.
		END CASE.

		textBox1:Text = descriptn.
	END METHOD.
