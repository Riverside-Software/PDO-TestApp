		else if type-of(tool,Infragistics.Win.UltraWinToolbars.FontListTool) then
        do:
            fontList = cast(tool,Infragistics.Win.UltraWinToolbars.FontListTool).
            fLabelManager:Font = new System.Drawing.Font(fontList:Value:toString(),10.0).
	    end. 
		else if type-of(tool,Infragistics.Win.UltraWinToolbars.ComboBoxTool) then
        do:
		    align = cast(tool,Infragistics.Win.UltraWinToolbars.ComboBoxTool).
		    fLabelManager:AlignLeft = (align:Value:toString() = "Left")    .
		end.    
	 
		RETURN.

	END METHOD.

END CLASS.