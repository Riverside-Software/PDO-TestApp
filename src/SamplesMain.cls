	METHOD PRIVATE VOID SamplesMain_Load( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        tvwSamples:ExpandAll().
        tvwSamples:Sort().
        tvwSamples:SelectedNode = tvwSamples:Nodes[0].
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PUBLIC VOID LaunchSample(  ):
		DEFINE VARIABLE sample     AS Progress.Windows.Form NO-UNDO.
		DEFINE VARIABLE objectType AS CHARACTER             NO-UNDO.

        THIS-OBJECT:CURSOR = System.Windows.Forms.Cursors:WaitCursor.

		objectType = ENTRY(1,STRING(tvwSamples:SelectedNode:Tag)).
    
		if objectType begins "WinGrid." then 
		do:
		    if not valid-object(fGridSamples) or fGridSamples:isDisposed then
               assign
                  fGridSamples = new WinGrid.samplesExplorer.frmMain()
                  sample = fGridSamples.    
		    else 
                fGridSamples:BringToFront().             
        end.    
        else
		sample = DYNAMIC-NEW objectType() no-error.
		
		if valid-object(sample) then
		    WAIT-FOR sample:Show(). 
	    
	    FINALLY:
		    THIS-OBJECT:CURSOR = System.Windows.Forms.Cursors:DEFAULT.          		
        END FINALLY.
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:  																	  
			Notes:  																	  
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID tvwSamples_AfterSelect( INPUT sender AS System.Object, INPUT e AS System.Windows.Forms.TreeViewEventArgs ):
	    DEFINE VARIABLE className    AS CHARACTER NO-UNDO.
	    DEFINE VARIABLE readmeFile   AS CHARACTER NO-UNDO.
	    DEFINE VARIABLE previewImage AS CHARACTER NO-UNDO.
	    DEFINE VARIABLE nodeTag      AS CHARACTER NO-UNDO.

        ASSIGN nodeTag = STRING(tvwSamples:SelectedNode:Tag).
		IF nodeTag = "root":U OR nodeTag = "" THEN
        DO:
            btnLaunchSample:ENABLED = FALSE.
            lblSampleName:TEXT = "".
            txtSampleDescription:Text = "".
            picSample:IMAGE = ?.
            RETURN.
        END.

        className    = ENTRY(1,nodeTag).
        previewImage = SEARCH("LaunchPreviews/" + ENTRY(2,nodeTag)).
       
        IF NUM-ENTRIES(nodeTag) > 2 AND ENTRY(3,nodeTag) NE "" THEN
            readmeFile = SEARCH(ENTRY(3,nodeTag)).
        ELSE
            readmeFile = SEARCH(REPLACE(SUBSTRING(className, 1, R-INDEX(className, ".") - 1), ".", "/") + "/readme.out").

        btnLaunchSample:ENABLED = TRUE.
        
        lblSampleName:TEXT = (IF tvwSamples:SelectedNode:Parent = ? THEN "" ELSE tvwSamples:SelectedNode:Parent:Text + "-") + tvwSamples:SelectedNode:Text.

        /* Some files may have Unix line endings; some may have Windows. Make sure we convert them all to Windows. */
        txtSampleDescription:Text = replace(replace(System.IO.File:ReadAllText(readmeFile, System.Text.Encoding:Default), '~r~n', '~n'), '~n', System.Environment:NewLine) NO-ERROR.
        IF ERROR-STATUS:ERROR THEN
            txtSampleDescription:TEXT = lblSampleName:TEXT NO-ERROR.
        picSample:IMAGE = System.Drawing.Image:FromFile(previewImage).
        CATCH e1 AS Progress.Lang.Error :
            picSample:IMAGE = ?.
        END CATCH.
	END METHOD.

