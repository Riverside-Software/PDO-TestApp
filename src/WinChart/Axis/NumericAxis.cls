	@VisualDesigner.
	METHOD PRIVATE VOID NumericAxis_Load( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
	    /* retrieve the dataset table as a DataTable */
        salesrepOrderDT = fSalesrepService:DataTable('eSalesrep',"","").

        ultraChart1:Data:DataSource = salesrepOrderDT.
        ultraChart1:Data:DataBind().
        ultraChart1:BackgroundImage = System.Drawing.Image:FromFile(SEARCH("WinChart/images/chart_gray_bg.jpg")).

        numericAxisTypeEditor:Text = STRING(ultraChart1:Axis:Y:NumericAxisType).
		tickmarkStyleEditor:SelectedIndex = tickmarkStyleEditor:FindString(STRING(ultraChart1:Axis:Y:TickmarkStyle)).
		tickIntervalEditor:Value = ultraChart1:Axis:Y:TickmarkInterval.
		tickPercentageEditor:Value = ultraChart1:Axis:Y:TickmarkPercentage.
	END METHOD.

	DESTRUCTOR PUBLIC NumericAxis ( ):
		IF VALID-OBJECT(components) THEN DO:
			CAST(components, System.IDisposable):Dispose().
		END.
	END DESTRUCTOR.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PUBLIC VOID InitializeDataSource(  ):
        fSalesrepService = new DataServices.SalesrepOrderService().                  
        fSalesrepService:fetchData().
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID numericAxisTypeEditor_ValueChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
	    CASE numericAxisTypeEditor:TEXT:
		    WHEN "Logarithmic" THEN
			    ultraChart1:Axis:Y:NumericAxisType = NumericAxisType:Logarithmic.
			WHEN "Linear" THEN
			    ultraChart1:Axis:Y:NumericAxisType = NumericAxisType:Linear.
		END CASE.
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID tickEditors_valueChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
	    ultraChart1:Axis:Y:TickmarkInterval   = UNBOX(tickIntervalEditor:Value).
		ultraChart1:Axis:Y:TickmarkPercentage = UNBOX(tickPercentageEditor:Value).
	END METHOD.

 	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID tickmarkStyleEditor_ValueChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        CASE tickmarkStyleEditor:TEXT:
            WHEN "Percentage" THEN
        		ASSIGN ultraChart1:Axis:Y:TickmarkStyle = AxisTickStyle:Percentage
        		tickIntervalEditor:Enabled = FALSE
        		tickPercentageEditor:Enabled = TRUE.
        	WHEN "DataInterval" THEN
        		ASSIGN ultraChart1:Axis:Y:TickmarkStyle = AxisTickStyle:DataInterval
        		tickIntervalEditor:Enabled = TRUE
        		tickPercentageEditor:Enabled = FALSE. 
        	WHEN "Smart" THEN
        		ASSIGN ultraChart1:Axis:Y:TickmarkStyle = AxisTickStyle:Smart
        		tickIntervalEditor:Enabled = TRUE
        		tickPercentageEditor:Enabled = FALSE.
        END CASE.
	END METHOD.
