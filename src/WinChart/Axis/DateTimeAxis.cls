	@VisualDesigner.
	METHOD PRIVATE VOID tickEditors_ValueChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        ultraChart1:Axis:X:TickmarkInterval   = UNBOX(tickIntervalEditor:Value).
        ultraChart1:Axis:X:TickmarkPercentage = UNBOX(tickPercentageEditor:Value).

        CASE tickIntervalTypeEditor:TEXT:
            WHEN "Ticks"        THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Ticks.
        	WHEN "Milliseconds" THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Milliseconds.
            WHEN "Seconds"      THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Seconds.
            WHEN "Minutes"      THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Minutes.
        	WHEN "Hours"        THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Hours.
        	WHEN "Days"         THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Days.
        	WHEN "Weeks"        THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Weeks.
        	WHEN "Months"       THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Months.
        	WHEN "Years"        THEN ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:Years.
            OTHERWISE ultraChart1:Axis:X:TickmarkIntervalType = AxisIntervalType:NotSet.
       END CASE.
	END METHOD.
 
