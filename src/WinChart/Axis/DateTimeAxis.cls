	METHOD PUBLIC VOID SetTicksStyle():
	    CASE tickmarkStyleEditor:Text:
		    WHEN "Percentage" THEN
            DO:
			    ultraChart1:Axis:X:TickmarkStyle = AxisTickStyle:Percentage.
				tickIntervalTypeEditor:Enabled = false.
				tickIntervalEditor:Enabled = false.
				tickPercentageEditor:Enabled = true.
			END.
		    WHEN "DataInterval" THEN
            DO:
			    ultraChart1:Axis:X:TickmarkStyle = AxisTickStyle:DataInterval.
				tickIntervalTypeEditor:Enabled = true.
				tickIntervalEditor:Enabled = true.
				tickPercentageEditor:Enabled = false.
			END.
			WHEN "Smart" THEN
            DO:
			    ultraChart1:Axis:X:TickmarkStyle = AxisTickStyle:Smart.
				tickIntervalTypeEditor:Enabled = false.
				tickIntervalEditor:Enabled = false.
				tickPercentageEditor:Enabled = false.
			END.
		END CASE.
	END METHOD.

