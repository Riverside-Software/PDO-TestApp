	@VisualDesigner.
	METHOD PRIVATE VOID frmFixedHeaders_Load( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
		/* Hide the grid caption by setting the Text to empty string. */
	    UltraGrid1:Text = "".

		UltraGrid1:DataSource = GetCustOrderSource().
		RETURN.

	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:  																	  
			Notes:  																	  
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID UltraGrid1_InitializeLayout( INPUT sender AS System.Object, INPUT e AS Infragistics.Win.UltraWinGrid.InitializeLayoutEventArgs ):
		/* Set the scroll style to immediate so the rows get scrolled immediately */
		/* when the vertical scrollbar thumb is dragged. */
	 
		e:Layout:ScrollStyle = Infragistics.Win.UltraWinGrid.ScrollStyle:Immediate.

		/* Turn on the fixed headers functionality. */
		 
		e:Layout:UseFixedHeaders = true.

		/* Fix some headers on various bands */
 
		e:Layout:Bands[0]:Columns["Custnum"]:Header:Fixed = true.

		e:Layout:Bands[1]:Columns["Custnum"]:Header:Fixed = true.
		e:Layout:Bands[1]:Columns["OrderNum"]:Header:Fixed = true.

		e:Layout:Bands[2]:Columns["OrderNum"]:Header:Fixed = true.
		e:Layout:Bands[2]:Columns["ItemNum"]:Header:Fixed = true.
		e:Layout:Bands[2]:Columns["LineNum"]:Header:Fixed = true.

		/* Set appearances for fixed headers and cells associated with fixed headers */
		 
		e:Layout:Override:FixedHeaderAppearance:BackColor = System.Drawing.Color:LightYellow.
		e:Layout:Override:FixedHeaderAppearance:ForeColor = System.Drawing.Color:Blue.
		e:Layout:Override:FixedCellAppearance:BackColor = System.Drawing.Color:LightYellow.			
		e:Layout:Override:FixedCellAppearance:ForeColor = System.Drawing.Color:Blue.

		/* Set the color of the line that seperates the fixed cells from the non-fixed cells. */
	 
		e:Layout:Override:FixedCellSeparatorColor = System.Drawing.Color:Blue.

		/* Initialize fields to their proper values. */
		 
		FillViewStyleCombo( ).
 		FillFixedHeaderIndicatorCombo( ). 
		chkFixedHeaders:Checked = e:Layout:UseFixedHeaders.
		ultraColorPicker1:Color = e:Layout:Override:FixedCellSeparatorColor.
        
        /* Expand the first band 0 row to reveal its first band 1 row. */
		if ( ultraGrid1:Rows:Count > 0 ) then
        do: 
            UltraGrid1:Rows[0]:Expanded = true.
            if ultraGrid1:Rows[0]:ChildBands[0]:Rows:Count > 0 then
                UltraGrid1:Rows[0]:ChildBands[0]:Rows[0]:Expanded = true.
        end.
        /*	UltraGrid1:Rows:ExpandAll( true ).*/
		RETURN.

	END METHOD.

