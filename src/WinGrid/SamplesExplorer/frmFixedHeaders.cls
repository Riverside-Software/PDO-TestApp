	@VisualDesigner.
	METHOD PRIVATE VOID comboBoxFixedHeaderIndicator_SelectedIndexChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
		UltraGrid1:DisplayLayout:Override:FixedHeaderIndicator = cast(comboBoxFixedHeaderIndicator:SelectedItem,Infragistics.Win.UltraWinGrid.FixedHeaderIndicator).
        
        /* InSwapDropDown fixed header indicator requires the swapping */
        /* functionality, so turn it on. */
		if Progress.Util.EnumHelper:AreEqual(Infragistics.Win.UltraWinGrid.FixedHeaderIndicator:InSwapDropDown,UltraGrid1:DisplayLayout:Override:FixedHeaderIndicator) then
			UltraGrid1:DisplayLayout:Override:AllowColSwapping = Infragistics.Win.UltraWinGrid.AllowColSwapping:WithinBand.
		else
			UltraGrid1:DisplayLayout:Override:AllowColSwapping = Infragistics.Win.UltraWinGrid.AllowColSwapping:Default.
	
		RETURN.

	END METHOD.
	
