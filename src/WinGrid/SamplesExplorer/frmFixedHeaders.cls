    method private void FillFixedHeaderIndicatorCombo( ):
		define variable i as integer no-undo.
	    define variable enumValues as System.Array no-undo.
	    define variable type as System.Type no-undo.
        comboBoxFixedHeaderIndicator:Items:Clear( ).
		
		type = Progress.Util.TypeHelper:GetType("Infragistics.Win.UltraWinGrid.FixedHeaderIndicator"). 
        enumValues = System.Enum:GetValues(type).
		do i = 0 to enumValues:Length - 1:                                                                                                       
            comboBoxFixedHeaderIndicator:Items:Add(enumValues:GetValue(i)).	
        end.
		comboBoxFixedHeaderIndicator:SelectedItem = comboBoxFixedHeaderIndicator:Items[0].
	end method.
    
    method private void FillViewStyleCombo( ):
		define variable i as integer no-undo.
	    define variable enumValues as System.Array no-undo.
	    define variable type as System.Type no-undo.
        comboBoxViewStyle:Items:Clear( ).
		
		type = Progress.Util.TypeHelper:GetType("Infragistics.Win.UltraWinGrid.ViewStyleBand"). 
        enumValues = System.Enum:GetValues(type).
		do i = 0 to enumValues:Length - 1:                                                                                                       
            comboBoxViewStyle:Items:Add(enumValues:GetValue(i)).	
        end.
		comboBoxViewStyle:SelectedItem = Infragistics.Win.UltraWinGrid.ViewStyleBand:Vertical.
	end method.
    
