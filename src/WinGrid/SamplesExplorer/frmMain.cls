    method private void UpdateDisplay():
		define variable pSample as ISamplesBase no-undo.
		define variable prevPicture as System.Drawing.Image no-undo.
        /* update the display based on the selected node */
		if valid-object(tvwSamples:SelectedNode) then
        do:
			case tvwSamples:SelectedNode:Text:
				when KEY_EXTENSIBILITYSAMPLES then 
				do:
				 	btnLaunchSample:Enabled = false.
					txtSampleDescription:Enabled = true.
					txtSampleDescription:Text = "These samples demonstrate powerful extensibility " +
							"features of the UltraGrid control. These samples show how you can extend " +
							"the UltraGrid with your own code.".

					lblSampleName:Enabled = true.
					lblSampleName:Text = tvwSamples:SelectedNode:Text.
				end.
				when KEY_FEATURESAMPLES then
                do:
            		btnLaunchSample:Enabled = false.
					txtSampleDescription:Enabled = true.

					txtSampleDescription:Text = "These samples display " +
						"specific features of the UltraGrid control.".
					
					lblSampleName:Enabled = true.
					lblSampleName:Text = tvwSamples:SelectedNode:Text.
                end.    
                when KEY_REALWORLDAPPS then
                do:
					btnLaunchSample:Enabled = false.
					txtSampleDescription:Enabled = true.

					txtSampleDescription:Text = "These samples show how " +
						"you can use the UltraGrid control in real " +
						"world applications.".

					lblSampleName:Enabled = true.
					lblSampleName:Text = tvwSamples:SelectedNode:Text.
                end.
                /* If the "Introduction" node is selected.... (not implemented) */
				when KEY_INTRODUCTION then
				do:
					btnLaunchSample:Enabled = false.
					txtSampleDescription:Enabled = true.

					txtSampleDescription:Text = 
						"The Samples Explorer " + 
						"provides a centralized way of viewing all of the samples " + 
						"for the UltraGrid control.\r\n\r\n" + 
						"The samples are split into three basic groups:" + "\r\n" + 
						"'Feature Samples' - display specific " + 
						"features of the UltraGrid control." + "\r\n" + 
						"'Extensibility Samples' - demonstrates powerful extensibility " + 
						"features of the UltraGrid control using your own code." + "\r\n" + 
						"'Real World Applications' - " + 
						"shows how the UltraGrid control can be " + 
						"used in real world applications.".

					lblSampleName:Enabled = true.
					lblSampleName:Text = tvwSamples:SelectedNode:Text.
                end.
                otherwise do:
/*                    pSample = tvwSamples:SelectedNode:Tag.*/
                    pSample = GetProgressObj(tvwSamples:SelectedNode:Tag).
                    if valid-object(pSample) then
                    do:
						btnLaunchSample:Enabled = true.
						txtSampleDescription:Enabled = true.

						txtSampleDescription:Text = pSample:Description.
						lblSampleName:Text = pSample:SampleName.

						prevPicture = pSample:PreviewPicture.
						
						lblSampleName:Enabled = true.
					end.
					else
                    do:
						btnLaunchSample:Enabled = false.
						txtSampleDescription:Enabled = false.

						txtSampleDescription:Text = "".
						lblSampleName:Text = "".

						lblSampleName:Enabled = false.
                    end.
                end.    
		    end case.
		end.	
		if not valid-object(prevPicture) then
		    assign
			    picSample:Image = ?
			    picSample:Visible = false.
			
		else
		    assign
			    picSample:Image = prevPicture.
    		    picSample:Visible = true.
			
    end method.
    
    method private void AddSampleToList(pSample as ISamplesBase ):
		define variable objNode as System.Windows.Forms.TreeNode no-undo.
		define variable xx as Progress.Lang.Object.
		if not valid-object(pSample) then
			return.

		if pSample:SampleName = "" then
			return.
        
        case pSample:SampleType:
            when "Extensibility" then 
            do:
				objNode = tvwSamples:Nodes[Index_Extensibility]:Nodes:Add(pSample:SampleName).
/*				objNode:Tag = pSample.*/
				objNode:Tag = new ObjectWrapper(pSample). 
			    objNode:ImageIndex = BUTTON_VERMILLION.
				objNode:SelectedImageIndex = BUTTON_VERMILLION.
			end.
			when "Feature" then
            do:
				objNode = tvwSamples:Nodes[Index_Feature]:Nodes:Add(pSample:SampleName).
 			 
 				objNode:Tag = new ObjectWrapper(pSample). 
				objNode:ImageIndex = BUTTON_VERMILLION.
				objNode:SelectedImageIndex = BUTTON_VERMILLION.		 
            end.
 			when "RealWorldApp" then
            do:
				objNode = tvwSamples:Nodes[Index_Realworld]:Nodes:Add(pSample:SampleName).
/*				objNode:Tag = pSample.*/
				objNode:Tag = new ObjectWrapper(pSample). 
				objNode:ImageIndex = BUTTON_BLUE.
				objNode:SelectedImageIndex = BUTTON_BLUE. 
            end.
            when "Introduction:" then
            do:
				objNode = tvwSamples:Nodes[Index_Introduction]:Nodes:Add(pSample:SampleName).
/*				objNode:Tag = pSample.*/
				objNode:Tag = new ObjectWrapper(pSample). 
				objNode:ImageIndex = BUTTON_BLUE.
				objNode:SelectedImageIndex = BUTTON_BLUE.
			end.
		end.	
	end method.

    method private void LaunchSample():
		define variable pSample as ISamplesBase no-undo.
    	/* if the selected node is a sample node, show it */
		if not valid-object(tvwSamples:SelectedNode:Parent) then
			return.

/*	    pSample = tvwSamples.SelectedNode.Tag.*/
        pSample = getProgressObj(tvwSamples:SelectedNode:Tag).
        if not valid-object(pSample) then
			return.

		pSample:ShowMe(this-object).
    end.
	
	method protected ISamplesBase GetProgressObj(obj as System.Object):	   
	   return cast(cast(obj,ObjectWrapper):ProgressObject,ISamplesBase).
	end. 
	
	/* Navigation to next sample */
	method public void Navigation( button as System.Windows.Forms.MouseButtons):
		/* called by every sample form lblTitle_MouseUp */
        define variable pSample as ISamplesBase no-undo.
     	pSample = GetProgressObj(tvwSamples:SelectedNode:Tag).
        if not valid-object(pSample) then
			return.
			
		do on error undo, throw:
        	if Progress.Util.EnumHelper:AreEqual(button,System.Windows.Forms.MouseButtons:Left) then
            do:	
                if not valid-object(tvwSamples:SelectedNode:PrevNode) then
    				return.
    
    			tvwSamples:SelectedNode = tvwSamples:SelectedNode:PrevNode.
    			if btnLaunchSample:Enabled then
    				LaunchSample().
    		end.
            else if Progress.Util.EnumHelper:AreEqual(button,System.Windows.Forms.MouseButtons:Right) then
            do:
                if not valid-object(tvwSamples:SelectedNode:NextNode) then
    				return.
    				
    			tvwSamples:SelectedNode = tvwSamples:SelectedNode:NextNode.
    				
    			if btnLaunchSample:Enabled then
    				LaunchSample().
    	    end.
    		if valid-object(pSample) then
    			pSample:HideMe().        
    			   
		    catch e as Progress.Lang.Error :
            	undo, throw e.	
            end catch.
		end.

	end method.	
		
 		
