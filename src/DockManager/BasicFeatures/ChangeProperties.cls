	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID ultraButton1_Click( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
	    openFileDialog1:Filter = "XML Files (*.xml)|*.xml|All Files (*.*)|*.*".
	    openFileDialog1:FILENAME = "".
		WAIT-FOR openFileDialog1:ShowDialog().
		dockManager:LoadFromXML(SEARCH(openFileDialog1:FileName)).
		
		CATCH errorLoadingXMLFile AS Progress.Lang.Error :
		    IF openFileDialog1:FILENAME NE "" THEN
                MESSAGE "The file '" + openFileDialog1:FILENAME + "' is not a valid XML file type to load a UltraDockManager layout."
                VIEW-AS ALERT-BOX ERROR
                TITLE "Error loading XML file".
        END CATCH.
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID ultraButton2_Click( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        saveFileDialog1:Filter = "XML Files (*.xml)|*.xml|All Files (*.*)|*.*".
        WAIT-FOR saveFileDialog1:ShowDialog().
        IF saveFileDialog1:FILENAME NE "" THEN
            dockManager:SaveAsXML(saveFileDialog1:FileName).
	END METHOD.
