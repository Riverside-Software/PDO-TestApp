	@VisualDesigner.
	METHOD PRIVATE VOID cbControls_ValueChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
		UpdateFields().
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PUBLIC VOID PopulateControlsCombo():
		DEFINE VARIABLE panesCollection AS DockableControlPanesCollection NO-UNDO.
		DEFINE VARIABLE totalPanes  AS INTEGER NO-UNDO.
		DEFINE VARIABLE currentPane AS INTEGER NO-UNDO.

		ASSIGN panesCollection = dockManager:ControlPanes
		       totalPanes      = panesCollection:COUNT - 1.

		REPEAT currentPane = 0 TO totalPanes:
		    cbControls:Items:Add(panesCollection[currentPane], panesCollection[currentPane]:CONTROL:name).
		END.

        IF totalPanes GE 0 THEN
        cbControls:SelectedIndex = 0.

	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID chPinned_CheckedChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
		CAST(cbControls:SelectedItem:datavalue,DockableControlPane):Pinned = chPinned:CHECKED.
		propertyGrid1:Refresh().
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PRIVATE VOID UpdateFields(  ):
		DEFINE VARIABLE selectedPane AS DockableControlPane NO-UNDO.

		selectedPane = CAST(cbControls:SelectedItem:datavalue,DockableControlPane).

		chPinned:CHECKED    = selectedPane:Pinned.
		chMinimized:CHECKED = selectedPane:Minimized.
		chFloat:CHECKED     = Progress.Util.EnumHelper:AreEqual(selectedPane:DockedState,DockedState:Floating).
		selectedPane:CONTROL:Focus().
		propertyGrid1:SelectedObject = selectedPane.
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID chMinimized_CheckedChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        CAST(cbControls:SelectedItem:datavalue,DockableControlPane):Minimized = chMinimized:CHECKED.
        propertyGrid1:Refresh().
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID chFloat_CheckedChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
	    IF chFloat:CHECKED THEN
           CAST(cbControls:SelectedItem:datavalue,DockableControlPane):Float(chFloat:checked).
        ELSE
           CAST(cbControls:SelectedItem:datavalue,DockableControlPane):Dock(TRUE).

        propertyGrid1:Refresh().
	END METHOD.

