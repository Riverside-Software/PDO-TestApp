	METHOD PRIVATE VOID InitializeCombo( cbo AS System.Windows.Forms.ComboBox, defaultEnum AS System.Enum):
        DEFINE VARIABLE enumType AS System.Type      NO-UNDO.
        DEFINE VARIABLE names    AS CHARACTER EXTENT NO-UNDO.
        DEFINE VARIABLE enumItem AS System.Object    NO-UNDO.
        DEFINE VARIABLE pos      AS INTEGER          NO-UNDO.
        DEFINE VARIABLE element  AS INTEGER          NO-UNDO.

        IF cbo = ? THEN RETURN.

		enumType = defaultEnum:GetType().

		cbo:Items:Clear().

		names = System.Enum:GetNames(enumType).

		REPEAT element = 1 TO EXTENT(names):
		    enumItem = System.Enum:Parse(enumType, names[element]).
		    pos = cbo:Items:Add(enumItem).

			IF defaultEnum:Equals(enumItem) THEN
		        cbo:SelectedIndex = pos.
        END.
	END METHOD.

