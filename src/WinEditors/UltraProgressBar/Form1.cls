	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID timerAutoRun_Tick( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        DEFINE VARIABLE currentValue AS INTEGER NO-UNDO.
        DEFINE VARIABLE maxValue     AS INTEGER NO-UNDO.
        DEFINE VARIABLE minValue     AS INTEGER NO-UNDO.
        DEFINE VARIABLE step         AS INTEGER NO-UNDO.

		currentValue = ultraProgressBarHorizontal:VALUE.
		maxValue     = ultraProgressBarHorizontal:MAXIMUM.
		minValue     = ultraProgressBarHorizontal:MINIMUM.
		step         = ultraProgressBarHorizontal:Step.

		IF (currentValue = maxValue AND step > 0) OR (currentValue = minValue AND step < 0) THEN
		    ultraProgressBarHorizontal:Step = ultraProgressBarHorizontal:Step * -1.

		ultraProgressBarHorizontal:PerformStep().

		ultraProgressBarVertical:Value = ultraProgressBarHorizontal:Value.
	END METHOD.

