	@VisualDesigner.
	METHOD PRIVATE VOID checkBoxAutoRun_CheckedChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        IF checkBoxAutoRun:CHECKED THEN
		    timerAutoRun:Start().
		ELSE
			timerAutoRun:Stop().
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID checkBoxBackgroundImage_CheckedChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        IF checkBoxBackgroundImage:CHECKED THEN
        DO:
			InitializeImages().

			ultraProgressBarHorizontal:FillAppearance:ImageBackground = horizontalImage.
			ultraProgressBarVertical:FillAppearance:ImageBackground = verticalImage.
		END.
		ELSE DO:
		    ultraProgressBarHorizontal:FillAppearance:ImageBackground = ?.
			ultraProgressBarVertical:FillAppearance:ImageBackground = ?.
		END.

		ResetFillForeColor().

	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PRIVATE VOID InitializeImages(  ):
        IF verticalImage NE ? THEN RETURN.

		horizontalImage = System.Drawing.Bitmap:FromFile(SEARCH("WinEditors/UltraProgressBar/infragistics.gif")).
		verticalImage = System.Drawing.Bitmap:FromFile(SEARCH("WinEditors/UltraProgressBar/infragisticsvert.gif")).
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	METHOD PRIVATE VOID ResetFillForeColor(  ):
	    DEFINE VARIABLE foreColor AS System.Drawing.Color NO-UNDO.

		foreColor = System.Drawing.Color:EMPTY.

		IF checkBoxThemes:Checked AND Infragistics.Win.XPThemes:ThemesSupported THEN
		    foreColor = System.Drawing.Color:EMPTY.
		ELSE IF checkBoxBackgroundImage:CHECKED THEN
			foreColor = System.Drawing.Color:Violet.
		ELSE IF checkBoxGradient:CHECKED THEN
			foreColor = System.Drawing.Color:Yellow.

		ultraProgressBarHorizontal:FillAppearance:ForeColor = foreColor.
		ultraProgressBarVertical:FillAppearance:ForeColor   = foreColor.
	END METHOD.

	/*------------------------------------------------------------------------------
			Purpose:
			Notes:
	------------------------------------------------------------------------------*/
	@VisualDesigner.
	METHOD PRIVATE VOID checkBoxGradient_CheckedChanged( INPUT sender AS System.Object, INPUT e AS System.EventArgs ):
        IF checkBoxGradient:CHECKED THEN
        DO:
			ultraProgressBarHorizontal:FillAppearance:BackGradientStyle = Infragistics.Win.GradientStyle:Horizontal.
			ultraProgressBarVertical:FillAppearance:BackGradientStyle = Infragistics.Win.GradientStyle:Vertical.
		END.
		ELSE DO:
