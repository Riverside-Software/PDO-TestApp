		trackbar = CAST(sender,System.Windows.Forms.TrackBar).

		IF trackbar = ? THEN RETURN.

		ultraProgressBarVertical:VALUE   = trackbar:VALUE.
		ultraProgressBarHorizontal:Value = trackbar:VALUE.
	END METHOD.

